{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "env": {
    "mocha": true,
    "shared-node-browser": true
  },
  "rules": {
    "quotes": [2, "single"],
    "func-names": 0,
    "key-spacing": 0,
    "new-cap": 0,
    "no-param-reassign": 0,
    "comma-dangle": [2, "never"],
    "no-new": 0,
    "radix": 0,
    "import/no-extraneous-dependencies": 0
  }
}

{
  "name": "httpmark-agent",
  "version": "0.0.1",
  "private": true,
  "description": "The test agent for httpmark",
  "main": "index.js",
  "engines": {
    "node": "7.2.0"
  },
  "scripts": {
    "test:server": "mocha server/**/*.spec.js",
    "test:extension": "mocha chrome-extension/**/*.spec.js --require chrome-extension/test-helper",
    "pretest": "npm run lint",
    "test": "npm run test:server && npm run test:extension",
    "compile-scripts": "babel-node $(npm bin)/webpack",
    "build": "npm run compile-scripts && cd chrome-extension && npm run build",
    "postbuild": "cp -r ./chrome-extension/build ./build/chrome-extension",
    "lint": "eslint --cache ."
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "rimraf": "^2.5.4",
    "which": "^1.2.11",
    "ws": "^1.1.1",
    "xvfb": "^0.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-es2015-destructuring": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "glob": "^7.1.1",
    "mocha": "^3.1.2",
    "ramda": "^0.22.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.3",
    "webpack-node-externals": "^1.5.4"
  }
}
